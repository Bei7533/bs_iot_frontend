alhost:8088/admin/deviceinfo', {\r\n          username: this.username,\r\n        })\r\n        .then(successResponse => {\r\n          if (successResponse.data.code === 200) {\r\n            this.deviceNum = successResponse.data.message;\r\n            this.stringList = successResponse.data.stringList;\r\n            for (let i = 0; i < this.deviceNum; i++) {\r\n              let device_str = this.stringList[i];\r\n              this.tableData.push({\r\n                id: device_str.substring(device_str.indexOf('id') + 3, device_str.indexOf(',')),\r\n                device_name: device_str.substring(device_str.indexOf('device_name') + 12, device_str.indexOf('device_type') - 2),\r\n                device_type: device_str.substring(device_str.indexOf('device_type') + 12, device_str.indexOf('device_discript') - 2),\r\n                device_discript: device_str.substring(device_str.indexOf('device_discript') + 16, device_str.indexOf('create_time') - 2),\r\n                create_time: device_str.substring(device_str.indexOf('create_time') + 12, device_str.length - 2),\r\n              });\r\n              console.log(device_str.substring(device_str.indexOf('id') + 3, device_str.indexOf(',')))\r\n              console.log(\"11111\")\r\n            }\r\n          } else if (successResponse.data.code === 404) {\r\n            alert('访问用户失败')\r\n          }\r\n        })\r\n        .catch(failResponse => {\r\n        });\r\n    },\r\n\r\n\r\n    handleSelect(key, keyPath) {\r\n      if (key === '1-1') {\r\n        // alert('1-1')\r\n        this.$router.push({\r\n          path: '/user/userinfo',\r\n          query: { username: this.username }\r\n        });\r\n      } else if (key === '1-2') {\r\n        this.$router.push({\r\n          path: '/user/userinfoedit',\r\n          query: { username: this.username }\r\n        });\r\n      } else if (key === '2') {\r\n        this.$router.push({\r\n          path: '/user/map',\r\n          query: { username: this.username }\r\n        });\r\n      } else if (key === '3') {\r\n        // this.$router.push({\r\n        //   path: '/user/device',\r\n        //   query: { username: this.username }\r\n        // });\r\n      } else if (key === '4') {\r\n        this.$router.push({\r\n          path: '/user/data',\r\n          query: { username: this.username }\r\n        });\r\n      }\r\n    },\r\n\r\n    handleClick(row) {\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div class=\"body\">\r\n\r\n    <el-menu :default-active=\"activeIndex2\" class=\"el-menu-demo\" mode=\"horizontal\" @select=\"handleSelect\"\r\n      background-color=\"#2A7A88\" text-color=\"#fff\" active-text-color=\"#ffd04b\">\r\n      <el-submenu index=\"1\">\r\n        <template slot=\"title\">\r\n          <i class=\"el-icon-user\"></i>\r\n          <span>个人信息</span>\r\n        </template>\r\n        <el-menu-item index=\"1-1\">查看信息</el-menu-item>\r\n        <el-menu-item index=\"1-2\">编辑信息</el-menu-item>\r\n      </el-submenu>\r\n      <el-menu-item index=\"2\">\r\n        <template slot=\"title\">\r\n          <i class=\"el-icon-map-location\"></i>\r\n          <span slot=\"title\">查看地图</span>\r\n        </template>\r\n      </el-menu-item>\r\n      <el-menu-item index=\"3\">\r\n        <i class=\"el-icon-setting\"></i>\r\n        <span slot=\"title\">设备管理</span>\r\n      </el-menu-item>\r\n      <el-menu-item index=\"4\">\r\n        <i class=\"el-icon-data-line\"></i>\r\n        <span slot=\"title\">数据统计</span>\r\n      </el-menu-item>\r\n    </el-menu>\r\n    <h1 class=\"username-style\">{{ username }}</h1>\r\n    <div class=\"demo-basic--circle\" style=\"position: absolute; top: 12px; right: 150px;\">\r\n      <div class=\"block\"><el-avatar size=\"40\" icon=\"el-icon-user-solid\"></el-avatar></div>\r\n      <div class=\"block\" v-for=\"size in sizeList\" :key=\"size\">\r\n        <el-avatar size=\"size\" icon=\"el-icon-user-solid\"></el-avatar>\r\n      </div>\r\n    </div>\r\n\r\n    <el-table class=\"table-style\" :data=\"tableData\" height=\"400\" border>\r\n      <el-table-column fixed prop=\"id\" label=\"设备ID\" width=\"130\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"device_name\" label=\"设备名称\" width=\"130\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"device_type\" label=\"设备类型\" width=\"130\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"device_discript\" label=\"设备描述\" width=\"300\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"create_time\" label=\"创建时间\" width=\"190\">\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"text\">查看设备历史信息</el-button>\r\n          <br>\r\n          <el-button @click=\"handleClick(scope.row)\" type=\"text\">编辑设备</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div class=\"top-style\">\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"6\">\r\n          <div>\r\n            <el-statistic group-separator=\",\" :precision=\"2\" :value=\"value2\" :title=\"title\"></el-statistic>\r\n          </div>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <div>\r\n            <el-statistic title=\"男女比\">\r\n              <template slot=\"formatter\">\r\n                456/2\r\n              </template>\r\n            </el-statistic>\r\n          </div>\r\n        </el-col>\r\n        <el-col :span=\"6\">\r\n          <div>\r\n            <el-statistic group-separator=\",\" :precision=\"2\" decimal-separator=\".\" :value=\"value1\" :title=\"title\">\r\n              <template slot=\"prefix\">\r\n                <i class=\"el-icon-s-flag\" style=\"color: red\"></i>\r\n              </template>\r\n              <template slot=\"suffix\">\r\n                <i class=\"el-icon-s-flag\" style=\"color: blue\"></i>\r\n              </template>\r\n            </el-statistic>\r\n          </div>\r\n        </el-col>\r\n        <el-col :span=\"6\">\r\n          <div>\r\n            <el-statistic :value=\"like ? 521 : 520\" title=\"Feedback\">\r\n              <template slot=\"suffix\">\r\n                <span @click=\"like = !like\" class=\"like\">\r\n                  <i class=\"el-icon-star-on\" style=\"color:red\" v-show=\"!!like\"></i>\r\n                  <i class=\"el-icon-star-off\" v-show=\"!like\"></i>\r\n                </span>\r\n              </template>\r\n            </el-statistic>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.body {\r\n  position: absolute;\r\n  top: 0px;\r\n  left: 0px;\r\n  right: 0px;\r\n  bottom: 0px;\r\n  width: auto;\r\n  height: auto;\r\n  z-index: 0;\r\n}\r\n\r\n.username-style {\r\n  position: absolute;\r\n  top: 2px;\r\n  right: 65px;\r\n  font-size: 25px;\r\n  color: #fff;\r\n}\r\n\r\n.table-style {\r\n  position: absolute;\r\n  top: 30%;\r\n  left: 15%;\r\n  width: auto;\r\n  float: center;\r\n  font-family: 'Exo', sans-serif;\r\n  font-size: 16px;\r\n}\r\n\r\n.top-style {\r\n  position: absolute;\r\n  top: 18%;\r\n  left: 10%;\r\n  width: 80%;\r\n  float: center;\r\n  font-family: 'Exo', sans-serif;\r\n  font-size: 28px;\r\n}\r\n\r\n.like {\r\n  cursor: pointer;\r\n  font-size: 25px;\r\n  display: inline-block;\r\n}\r\n</style>\r\n"],"mappings":";AACA;EACAA,KAAA;IACA;MACAC,QAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAF,QAAA;MACAG,WAAA;MACAC,YAAA;MACAC,SAAA;MACAC,WAAA;MACAC,SAAA;MACAC,UAAA;MACAC,IAAA;MACAC,MAAA;MACAC,MAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,SAAA;EACA;;EACAC,OAAA;IACAD,UAAA;MACA,IAAAE,IAAA;MACA,KAAAC,MAAA,CACAC,IAAA;QACAlB,QAAA,OAAAA;MACA,GACAmB,IAAA,CAAAC,eAAA;QACA,IAAAA,eAAA,CAAArB,IAAA,CAAAsB,IAAA;UACA,KAAAhB,SAAA,GAAAe,eAAA,CAAArB,IAAA,CAAAuB,OAAA;UACA,KAAAd,UAAA,GAAAY,eAAA,CAAArB,IAAA,CAAAS,UAAA;UACA,SAAAe,CAAA,MAAAA,CAAA,QAAAlB,SAAA,EAAAkB,CAAA;YACA,IAAAC,UAAA,QAAAhB,UAAA,CAAAe,CAAA;YACA,KAAAhB,SAAA,CAAAkB,IAAA;cACAC,EAAA,E