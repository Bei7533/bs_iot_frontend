ta.code === 200) {\r\n            // 回到登录界面\r\n            this.$router.push({\r\n              path: '/'\r\n            })\r\n          }\r\n        }).catch(res => {\r\n          alert(res.data.msg)\r\n        })\r\n      } else {\r\n        alert('两次输入的密码不同')\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div>\r\n    <div class = \"body\">\r\n      <div class=\"login-register\">\r\n        <h2 class=\"register-title\">Register</h2>\r\n        <router-link to=\"/?index=1\" class=\"login-box\">\r\n          <link>Login</link>\r\n        </router-link>\r\n      </div>\r\n\r\n      <div class = \"box txt-type\" style=\"text-align: right;\">\r\n        <div style=\"  padding: 40px;\">\r\n      用户名 : <input id=\"userName\" type=\"text\" v-model=\"userName\" placeholder=\" 请输入用户名\"  class = \"input-type\" />\r\n      <br><br>\r\n      邮箱 : <input id=\"email\" type=\"text\" v-model=\"email\" placeholder=\" 请输入邮箱号\" class = \"input-type\" />\r\n        <br><br>\r\n      密码 : <input id=\"password\" type=\"password\" v-model=\"password\" placeholder=\" 请输入密码\" class = \"input-type\"/>\r\n      <br><br>\r\n      确认密码 : <input id=\"password2\" type=\"password\" v-model=\"password2\" placeholder=\" 请再次输入密码\" class = \"input-type\" />\r\n      <br><br>\r\n      <button @click=\"register()\" class=\"register2-box\">Register</button>\r\n      <br><br>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.body{\r\n  position: absolute;\r\n  top: 0px;\r\n  left: 0px;\r\n  right: 0px;\r\n  bottom: 0px;\r\n  width: auto;\r\n  height: auto;\r\n  background-image: url(\"../../assets/logo.png\");\r\n  background-size: cover;\r\n  //-webkit-filter: blur(5px);\r\n  z-index: 0;\r\n}\r\n\r\n.login-register{\r\n  position: absolute;\r\n  top: calc(50% - 200px);\r\n  left: calc(50% - 550px);\r\n}\r\n\r\n.register-title {\r\n  position: relative;\r\n  z-index: 2;\r\n  float: left;\r\n  color: #fff;\r\n  font-family: 'Exo', sa