  });\n      } else if (key === '1-2') {\n        this.$router.push({\n          path: '/user/userinfoedit',\n          query: {\n            username: this.username\n          }\n        });\n      } else if (key === '2') {\n        this.$router.push({\n          path: '/user/map',\n          query: {\n            username: this.username\n          }\n        });\n      } else if (key === '3') {\n        // this.$router.push({\n        //   path: '/user/device',\n        //   query: { username: this.username }\n        // });\n      } else if (key === '4') {\n        this.$router.push({\n          path: '/user/data',\n          query: {\n            username: this.username\n          }\n        });\n      }\n    },\n    handleClick(row) {}\n  }\n};","map":{"version":3,"names":["data","username","$route","query","activeIndex","activeIndex2","deviceNum","deviceTable","tableData","stringList","mounted","fetchData","methods","flag","$axios","post","then","successResponse","code","message","alert","catch","failResponse","console","log","i","device_str","substring","index","push","id","device_name","device_type","device_discript","create_time","length","handleSelect","key","keyPath","$router","path","handleClick","row"],"sources":["src/views/device/Device.vue"],"sourcesContent":["<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      username: this.$route.query.username,\r\n      activeIndex: '1',\r\n      activeIndex2: '1',\r\n      deviceNum: '',\r\n      deviceTable: [],\r\n      tableData: [],\r\n      stringList: []\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchData(); // 在组件挂载后调用发送请求的方法\r\n  },\r\n  methods: {\r\n    fetchData() {\r\n      let flag = false\r\n      this.$axios\r\n        .post('http://localhost:8088/admin/deviceinfo', {\r\n          username: this.username,\r\n        })\r\n        .then(successResponse => {\r\n          if (successResponse.data.code === 200) {\r\n            this.deviceNum = successResponse.data.message;\r\n            this.stringList = successResponse.data.stringList;\r\n            flag = true\r\n          } else if (successResponse.data.code === 404) {\r\n            alert('访问用户失败')\r\n          }\r\n        })\r\n        .catch(failResponse => {\r\n        });\r\n\r\n      console.log('success')\r\n      for (let i = 0; i < this.deviceNum; i++) {\r\n        let device_str = this.stringList[i];\r\n        console.log(device_str.substring(device_str.index('id') + 3, device_str.index('user_name') - 1))\r\n        this.tableData.push({\r\n          id: device_str.substring(device_str.index('id') + 3, device_str.index('user_name') - 1),\r\n          device_name: device_str.substring(device_str.index('device_name') + 13, device_str.index('device_type') - 1),\r\n          device_type: device_str.substring(device_str.index('device_type') + 13, device_str.index('device_discript') - 1),\r\n          device_discript: device_str.substring(device_str.index('device_discript') + 16, device_str.index('create_time') - 1),\r\n          create_time: device_str.substring(device_str.index('create_time') + 13, device_str.length - 1),\r\n        });\r\n\r\n      }\r\n    },\r\n\r\n\r\n    handleSelect(key, keyPath) {\r\n      if (key === '1-1') {\r\n        // alert('1-1')\r\n        this.$router.push({\r\n          path: '/user/userinfo',\r\n          query: { username: this.username }\r\n        });\r\n      } else if (key === '1-2') {\r\n        this.$router.push({\r\n          path: '/user/userinfoedit',\r\n          query: { username: this.username }\r\n        });\r\n      } else if (key === '2') {\r\n        this.$router.push({\r\n          path: '/user/map',\r\n          query: { username: this.username }\r\n        });\r\n      } else if (key === '3') {\r\n        // this.$router.push({\r\n        //   path: '/user/device',\r\n        //   query: { username: this.username }\r\n        // });\r\n      } else if (key === '4') {\r\n        this.$router.push({\r\n          path: '/user/data',\r\n          query: { username: this.username }\r\n        });\r\n      }\r\n    },\r\n\r\n    handleClick(row) {\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div class=\"body\">\r\n\r\n    <el-menu :default-active=\"activeIndex2\" class=\"el-menu-demo\" mode=\"horizontal\" @select=\"handleSelect\"\r\n      background-color=\"#2A7A88\" text-color=\"#fff\" active-text-color=\"#ffd04b\">\r\n      <el-submenu index=\"1\">\r\n        <template slot=\"title\">\r\n          <i class=\"el-icon-user\"></i>\r\n          <span>个人信息</span>\r\n        </template>\r\n        <el-menu-item index=\"1-1\">查看信息</el-menu-item>\r\n        <el-menu-item index=\"1-2\">编辑信息</el-menu-item>\r\n      </el-submenu>\r\n      <el-menu-item index=\"2\">\r\n        <template slot=\"title\">\r\n          <i class=\"el-icon-map-location\"></i>\r\n          <span slot=\"title\">查看地图</span>\r\n        </template>\r\n      </el-menu-item>\r\n      <el-menu-item index=\"3\">\r\n        <i class=\"el-icon-setting\"></i>\r\n        <span slot=\"title\">设备管理</span>\r\n      </el-menu-item>\r\n      <el-menu-item index=\"4\">\r\n        <i class=\"el-icon-data-line\"></i>\r\n        <span slot=\"title\">数据统计</span>\r\n      </el-menu-item>\r\n    </el-menu>\r\n    <h1 class=\"username-style\">{{ username }}</h1>\r\n    <div class=\"demo-basic--circle\" style=\"position: absolute; top: 12px; right: 150px;\">\r\n      <div class=\"block\"><el-avatar size=\"40\" icon=\"el-icon-user-solid\"></el-avatar></div>\r\n      <div class=\"block\" v-for=\"size in sizeList\" :key=\"size\">\r\n        <el-avatar size=\"size\" icon=\"el-icon-user-solid\"></el-avatar>\r\n      </div>\r\n    </div>\r\n\r\n    <el-table class=\"table-style\" :data=\"tableData\" height=\"500\" border>\r\n      <el-table-column fixed prop=\"date\" label=\"设备ID\" width=\"150\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"name\" label=\"姓名\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"province\" label=\"省份\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"city\" label=\"市区\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"address\" label=\"地址\" width=\"300\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"zip\" label=\"邮编\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"text\">查看设备历史信息</el-button>\r\n          <br>\r\n          <el-button @click=\"handleClick(scope.row)\" type=\"text\">编辑设备</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.body {\r\n  position: absolute;\r\n  top: 0px;\r\n  left: 0px;\r\n  right: 0px;\r\n  bottom: 0px;\r\n  width: auto;\r\n  height: auto;\r\n  z-index: 0;\r\n}\r\n\r\n.username-style {\r\n  position: absolute;\r\n  top: 2px;\r\n  right: 65px;\r\n  font-size: 25px;\r\n  color: #fff;\r\n}\r\n\r\n.table-style {\r\n  position: absolute;\r\n  top: 30%;\r\n  left: 10%;\r\n  width: auto;\r\n  float: left;\r\n  font-family: 'Exo', sans-serif;\r\n  font-size: 16px;\r\n}\r\n</style>\r\n"],"mappings":";AACA;EACAA,KAAA;IACA;MACAC,QAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAF,QAAA;MACAG,WAAA;MACAC,YAAA;MACAC,SAAA;MACAC,WAAA;MACAC,SAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,SAAA;EACA;;EACAC,OAAA;IACAD,UAAA;MACA,IAAAE,IAAA;MACA,KAAAC,MAAA,CACAC,IAAA;QACAd,QAAA,OAAAA;MACA,GACAe,IAAA,CAAAC,eAAA;QACA,IAAAA,eAAA,CAAAjB,IAAA,CAAAkB,IAAA;UACA,KAAAZ,SAAA,GAAAW,eAAA,CAAAjB,IAAA,CAAAmB,OAAA;UACA,KAAAV,UAAA,GAAAQ,eAAA,CAAAjB,IAAA,CAAAS,UAAA;UACAI,IAAA;QACA,WAAAI,eAAA,CAAAjB,IAAA,CAAAkB,IAAA;UACAE,KAAA;QACA;MACA,GACAC,KAAA,CAAAC,YAAA,KACA;MAEAC,OAAA,CAAAC,GAAA;MACA,SAAAC,CAAA,MAAAA,CAAA,QAAAnB,SAAA,EAAAmB,CAAA;QACA,IAAAC,UAAA,QAAAjB,UAAA,CAAAgB,CAAA;QACAF,OAAA,CAAAC,GAAA,CAAAE,UAAA,CAAAC,SAAA,CAAAD,UAAA,CAAAE,KAAA,YAAAF,UAAA,CAAAE,KAAA;QACA,KAAApB,SAAA,CAAAqB,IA