2: '1',\r\n            editContent: '',\r\n            tableData: [\r\n                {\r\n                    type: '姓名',\r\n                    data: this.$route.query.username,\r\n                },\r\n                {\r\n                    type: '邮箱',\r\n                    data: this.email,\r\n                },\r\n                {\r\n                    type: '密码',\r\n                    data: this.password,\r\n                }\r\n            ],\r\n            editRow: '',\r\n            editIndex: 0,\r\n            typeList: [\"姓名\", \"邮箱\", \"密码\"],\r\n            dialogFormVisible: false,\r\n        }\r\n    },\r\n    mounted() {\r\n        this.fetchData();\r\n    },\r\n    methods: {\r\n        fetchData() {\r\n            this.$axios\r\n                .post('http://localhost:8088/admin/userinfo', {\r\n                    username: this.username,\r\n                })\r\n                .then(successResponse => {\r\n                    if (successResponse.data.code === 200) {\r\n                        this.email = successResponse.data.userInfo.email;\r\n                        this.password = successResponse.data.userInfo.password;\r\n                        this.tableData = [\r\n                            {\r\n                                type: '姓名',\r\n                                data: this.$route.query.username,\r\n                            },\r\n                            {\r\n                                type: '邮箱',\r\n                                data: this.email,\r\n                            },\r\n                            {\r\n                                type: '密码',\r\n                                data: this.password,\r\n                            }\r\n                        ]\r\n                    } else if (successResponse.data.code === 404) {\r\n                        alert('访问用户失败')\r\n                    }\r\n                })\r\n                .catch(failResponse => {\r\n                })\r\n        